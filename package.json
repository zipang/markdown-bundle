		"editor",
		"simplemde",
		"katex"
	],
	"homepage": "https://github.com/zipang/markdown-bundle",
	"main": "./src/index.js",
	"license": "MIT",
	"author": {
		"name": "zipang",
		"url": "https://github.com/zipang"
	},
	"bugs": {
		"url": "https://github.com/zipang/markdown-bundle/issues"
	},
	"scripts": {
